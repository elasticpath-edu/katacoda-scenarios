							"type": "text/javascript",
							"listen": "test",
							"script": {
								"id": "deb5a68d-cad6-401f-a9cb-cad2b188e489",
								"exec": [
									"const json = pm.response.json()",
									"const d = json.hasOwnProperty(\"data\") ? json.data : json",
									"if(Array.isArray(d)){",
									"for (i = 0; i < d.length; i++){",
									"const field = d[i]",
									"const value = field.id",
									"if (i == 0) pm.environment.set(\"fieldID\", value)",
									"tests[\"[INFO] Response: \" + JSON.stringify(field,null,\"\\t\")] = true",
									"}} else {",
									"const value = d.id",
									"pm.environment.set(\"fieldID\", value)",
